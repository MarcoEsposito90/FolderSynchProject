<mui:ModernWindow x:Class="FolderSynchMUIClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="FolderSynch" IsTitleVisible="True"
        LogoData=""
        ContentSource="/Pages/UserPages/LoginPage.xaml"
        >

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Welcome to FolderSynch" GroupKey="login">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Login" Source="/Pages/UserPages/LoginPage.xaml" />
                <mui:Link DisplayName="Register" Source="/Pages/UserPages/RegisterPage.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
